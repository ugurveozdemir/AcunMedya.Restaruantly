<section id="about" class="about">
    <div class="container" data-aos="fade-up">

        <div class="row">
            <div class="col-lg-6 order-1 order-lg-2" data-aos="zoom-in" data-aos-delay="100">
                <div class="about-img">
                    <img src="@Url.Content(ViewBag.AboutImage)" alt="AboutImage" />
                </div>
            </div>
            <div class="col-lg-6 pt-4 pt-lg-0 order-2 order-lg-1 content">
                <h3>@ViewBag.AboutTitle</h3>
                
                <ul>
                    <li><i class="bi bi-check-circle"></i> @ViewBag.AboutFitem</li>
                    <li><i class="bi bi-check-circle"></i> @ViewBag.AboutSitem</li>
                    <li><i class="bi bi-check-circle"></i>@ViewBag.AboutTitem</li>
                </ul>
                <p>
                    @ViewBag.AboutDescription
                </p>
            </div>
        </div>

    </div>
</section>
