<div class="col-lg-4">
    <div class="info">
        <div class="address">
            <i class="bi bi-geo-alt"></i>
            <h4>Location:</h4>
            <p>@ViewBag.AdressLocation</p>
        </div>

        <div class="open-hours">
            <i class="bi bi-clock"></i>
            <h4>Open Hours:</h4>
            <p>
                @ViewBag.AdressOpenHours
            </p>
        </div>

        <div class="email">
            <i class="bi bi-envelope"></i>
            <h4>Email:</h4>
            <p>@ViewBag.AdressEmail</p>
        </div>

        <div class="phone">
            <i class="bi bi-phone"></i>
            <h4>Call:</h4>
            <p>@ViewBag.AdressCall</p>
        </div>

    </div>

</div>